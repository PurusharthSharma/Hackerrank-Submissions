import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    public static void main(String args[]){
        int sum=0;
        int num;
        Scanner sys = new Scanner (System.in);
        num=sys.nextInt();
        for(int i=0;i<num;i++){
            sum=sum+(sys.nextInt());
        }
        System.out.println(sum);
    }

  
}
